name: submis-mlflow
 
conda_env: conda.yaml
 
entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 50}
      max_depth: {type: int, default: 100}
      dataset : {type: string, default: "train_data.csv"}
        command: "python modelling.py {n_neighbors} {leaf_size} {dataset}"