name: submis-mlflow

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_neighbors: {type: int, default: 50}
      leaf_size: {type: int, default: 100}
      dataset: {type: string, default: "train_data.csv"}
    command: "python modelling.py --n_neighbors {n_neighbors} --leaf_size {leaf_size} --dataset {dataset}"
